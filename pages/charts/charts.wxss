@import"font/iconfont.wxss";
page{
  height: 100%;
  width: 100%;
}

.canvas-contain{
  height: 100%;
  width: 100%;
}

.date-show{
    position: relative;
    width: 600rpx;
    height: 100rpx;
    font-family: PingFang-SC-Regular;
    font-size: 38rpx;
    color: #282828;
    text-align: center;
    margin: 15rpx auto 0rpx;
}

.date-show icon{
   padding: 0 50rpx;
}

.date-text{
  width:300rpx;
  display: inline-block;
}

canvas{
  width: 100%;
  height: 280px;
}

text{
  color: #7C7C7C;
  font-size: 13px;
  line-height: 15px;
}

.text-contain{
  margin-top: 40px;
  padding-left: 25px;
  padding-right: 25px;
}